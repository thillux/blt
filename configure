$as_echo_n "checking whether declaration is needed for j1... " >&6; }
if ${blt_cv_need_decl_j1+:} false; then :
  $as_echo_n "(cached) " >&6
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#ifdef HAVE_STDLIB_H
