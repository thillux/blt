#endif /* HAVE_UNISTD_H */

_ACEOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  $EGREP "srand48" >/dev/null 2>&1; then :
  blt_cv_need_decl_srand48=no
else
  blt_cv_need_decl_srand48=yes
fi
rm -f conftest*

fi


if test "${blt_cv_need_decl_srand48}" = "yes"; then
    $as_echo "#define NEED_DECL_SRAND48 1" >>confdefs.h

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $blt_cv_need_decl_srand48" >&5
$as_echo "$blt_cv_need_decl_srand48" >&6; }

